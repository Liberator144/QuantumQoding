    // Function template
    const functionTemplate = {
        id: uuidv4(),
        name: 'TypeScript Function',
        description: 'A TypeScript function with parameters and return type',
        language: ProgrammingLanguage.TYPESCRIPT,
        category: TemplateCategory.FUNCTION,

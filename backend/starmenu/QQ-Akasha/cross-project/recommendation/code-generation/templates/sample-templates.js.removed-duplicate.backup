                type: VariableType.TYPE,
                description: 'The return type of the method',
                required: false,
                defaultValue: 'any',
            },
        ],
        tags: ['typescript', 'interface'],
        version: '1.0.0',
        author: 'Quantum Qoding',
        createdAt: new Date(),
        updatedAt: new Date(),
        usageCount: 0,
